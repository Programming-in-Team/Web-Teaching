{
    "markdown-pdf.convertOnSave": true,
    "markdown-pdf.convertOnSaveExclude": [
        "README.md",
        "index.md",
        "sample-exams.md",
        "vsc-usage.md",
        "nodeschool-exercises.md"
      ],    
      "markdown-pdf.outputDirectory": "generatedPDFs",
    "markdown-pdf.footerTemplate": "<div style=\"font-size: 9px; margin: 0 auto;\">CSE1500, Material developed by Claudia Hauff, Pages <span class='pageNumber'></span> / <span class='totalPages'></span></div>",
    "markdown-pdf.headerTemplate": "<div style=\"font-size: 9px; margin-left: 1cm;\"> <span class='title'></span></div> <div style=\"font-size: 9px; margin-left: auto; margin-right: 1cm; \"></div>",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    }
}